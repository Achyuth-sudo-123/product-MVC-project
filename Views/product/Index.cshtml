@model IEnumerable<productMVCproject.Models.productTable>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.BrandTable.Brand)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CategoriTable.Catogori)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.quentity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.price)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.BrandTable.Brand)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CategoriTable.Catogori)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.quentity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.price)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
